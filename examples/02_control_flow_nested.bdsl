;; Init var
x 0..10
y 5..20

;; Assign
z = x

;; Print bounds
z? ;; -> z ∈ (0, 10)

?? z >= 5
    ?? z > 8 
        z? ;; --> z ∈ (8, 10)
        z! = -1
        z? ;; --> z ∈ [-1, -1]
    >> 
        z? ;; --> z ∈ [5, 8)
        z! = -20 * x - y
        z? ;; --> z ∈ (-205, -20)
    --
    z? ;; --> z ∈ [-205, -20] ∪ [-1, -1]
>> ;; Else
    z? ;; --> z ∈ (0, 5]
    z! = x + y
    z? ;; --> z ∈ (5, 30)
--

;; Print final bounds
z? ;; --> z ∈ [-205, -20] ∪ [-1, -1] ∪ (5, 30)
